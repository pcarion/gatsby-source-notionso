,"properties":[]},{"blockId":"78d1b807-ec2a-4165-824c-ffdd88cbf3b1","blockIds":[],"type":"image","attributes":[{"att":"width","value":"419"},{"att":"aspectRatio","value":"0.8711217183770883"}],"properties":[{"propName":"source","value":[{"text":"https://s3-us-west-2.amazonaws.com/secure.notion-static.com/428a13d5-d5a0-4b89-926a-cb382fc44c34/Untitled.png","atts":[]}]}]},{"blockId":"70460865-015f-4d0e-b2de-e133356a549b","blockIds":[],"type":"text","attributes":[],"properties":[{"propName":"title","value":[{"text":"You can use the ","atts":[]},{"text":"Copy Page Link","atts":[{"att":"c","value":null}]},{"text":" to then copy the URL in your ","atts":[]},{"text":"gatsby-config.js","atts":[{"att":"c","value":null}]},{"text":" file in the ","atts":[]},{"text":"rootPageUrl","atts":[{"att":"c","value":null}]},{"text":" property.","atts":[]}]}]},{"blockId":"deaf021f-0471-4bcf-8843-6a90d127f7cf","blockIds":[],"type":"sub_header","attributes":[],"properties":[{"propName":"title","value":[{"text":"Content Page","atts":[]}]}]},{"blockId":"f67bd664-be11-48a4-a1c9-4115b3d40447","blockIds":[],"type":"text","attributes":[],"properties":[{"propName":"title","value":[{"text":"Once you have made your root page public, you can start adding content pages to that root page, using the \"+\" button","atts":[]}]}]},{"blockId":"c7df036e-c6ba-4023-b255-4ee55a1408d2","blockIds":[],"type":"text","attributes":[],"properties":[{"propName":"title","value":[{"text":" ","atts":[]}]}]},{"blockId":"837c9952-528d-42b2-8d0d-69435e02e06d","blockIds":[],"type":"text","attributes":[],"properties":[]},{"blockId":"fbffb973-a656-484a-a354-744f7d5bb73f","blockIds":[],"type":"image","attributes":[{"att":"width","value":"532"},{"att":"aspectRatio","value":"1.137218045112782"}],"properties":[{"propName":"source","value":[{"text":"https://s3-us-west-2.amazonaws.com/secure.notion-static.com/2c66b61c-c3db-4f77-b19c-4f7999b567d5/Untitled.png","atts":[]}]}]},{"blockId":"9df322f6-1d67-4bb5-b64e-bdef6a9c1839","blockIds":